{
  "extends": [
    "fbjs",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "jest"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "consistent-return": "off",
    "max-len": "off"
  },
  "env": {
    "node": true,
    "jest/globals": true
  }
}
