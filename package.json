{
  "name": "dawnjs",
  "version": "1.0.0",
  "description": "A client side rendering basic website starter kit with node and react",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:watch": "webpack --config webpack.config.js --watch --color --progress",
    "start": "node server",
    "start-dev": "npm run start-dev-server",
