{
  "name": "dawnjs",
  "version": "1.0.0",
  "description": "A client side rendering basic website starter kit with node and react",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --watch",
    "start-dev": "nodemon --config ./config/nodemon.json --exec npm run start-dev-server",
    "start-dev-server": "NODE_ENV=local node server",
    "start:dev": "npm-run-all --parallel build start-dev",
    "eslint": "eslint --ignore-path .gitignore .",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "keywords": [
    "dawnJs"
