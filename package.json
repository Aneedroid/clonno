    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "file-loader": "^2.0.0",
    "less-loader": "^4.1.0",
    "material-ui": "^0.20.2",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
